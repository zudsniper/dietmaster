<html>
   <head>
      <title>DietMaster Settings</title>
      <meta charset="utf-8">
     <meta http-equiv="X-UA-Compatible" content="IE=edge">
     <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- assets -->
    <!-- js -->
    <script
      src="https://code.jquery.com/jquery-3.6.0.min.js"
      integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4="
      crossorigin="anonymous">
    </script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js" integrity="sha384-B4gt1jrGC7Jh4AgTPSdUtOBvfO8shuf57BaghqFfPlYxofvL8/KUEfYiJOMMV+rV" crossorigin="anonymous"></script>
    <!-- css -->
    <link rel="stylesheet" href="stylesheets/style.css">
    <link rel="stylesheet" href="stylesheets/settings.css">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z" crossorigin="anonymous">
  </head>
  <body>
     <div class="container">
         <div class="row">
            <div id="main" class="col-lg-9 col-lg-pull-3 text-center">
               <h1>Notifications</h1>
               <img src="/images/dietmaster-logo.png">
            </div>
            <div class="col-9 col-push-3" style="font-size: 150%; padding-left: 10%; padding-bottom: 5%;">Push Notifications</div>
            <div class="col-3 col-pull-9">
               <label class="switch">
                  <input type="checkbox">
                  <span class="slider round"></span>
                </label>
            </div>
            <div class="col-9 col-push-3" style="font-size: 150%; padding-left: 10%; padding-bottom: 5%;">Email</div>
            <div class="col-3 col-pull-9">
               <label class="switch">
                  <input type="checkbox">
                  <span class="slider round"></span>
                </label>
            </div>
            <div class="col-9 col-push-3" style="font-size: 150%; padding-left: 10%; padding-bottom: 5%;">SMS</div>
            <div class="col-3 col-pull-9">
               <label class="switch">
                  <input type="checkbox">
                  <span class="slider round"></span>
                </label>
            </div>
            <div class="col-9 col-push-3" style="font-size: 150%; padding-left: 10%; padding-bottom: 5%;">Phone Calls</div>
            <div class="col-3 col-pull-9">
               <label class="switch">
                  <input type="checkbox">
                  <span class="slider round"></span>
                </label>
            </div>
            <div class="col-9 col-push-3" style="font-size: 150%; padding-left: 10%; padding-bottom: 5%;">Time</div>
            <div style="padding-left: 2%;">
               <input type="text" size="4">
            </div>
         </div>
         <div id="backnotif">
            <li class="list-group">
                  <ul class="list-group-item"><a href="/settings">Back</a></ul>
            </li>
         </div>
     </div>
  </body>
</html>